{
  "name": "my-custom-life-app",
  "version": "0.0.1",
  "author": "My Custom Life",
  "homepage": "http://mycustom.life/",
  "keywords": [
    "Caterpillar",
    "FastRent"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "@resources-ios": "cordova-res ios && node scripts/resources-ios.js",
    "@resources-android": "cordova-res android && node scripts/resources-android.js",
    "@browser-dev": "npx cap serve",
    "@add-android-platform": "npx cap add android",
    "@remove-android-platform": "rm -rf android/",
    "@add-ios-platform": "npx cap add ios",
    "@remove-ios-platform": "rm -rf ios/",
    "@build-ios": "ionic build && npx copy ios && npx cap open ios",
    "@build-prod": "ionic build --prod --release",
    "@update-android-capacitor": "npx copy android && npx cap open android",
    "@update-ios-capacitor": "npm run @resources-ios && npx copy ios && npx cap open ios",
    "@build-android": "npm run @build-prod && npm run @update-capacitor",
    "@build-android-prod": "npm run @build-prod && npm run @remove-android-platform && npm run @add-android-platform && npm run @update-android-capacitor",
    "@update-pod-ios": "cd ios/App/ && pod update",
    "@build-init-ios-prod": "npm run @build-prod && npm run @add-ios-platform && npm run @update-pod-ios && npm run @update-ios-capacitor",
    "@build-ios-prod": "npm run @build-prod && npm run @update-ios-capacitor",
    "@firebase-build-prod": "npm run @build-prod && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@capacitor/android": "2.0.1",
    "@capacitor/core": "^2.0.1",
    "@capacitor/ios": "2.0.1",
    "@ionic-native/core": "^5.0.7",
    "@ionic-native/firebase": "^5.24.0",
    "@ionic-native/firebase-analytics": "^5.24.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "cordova-plugin-firebase-analytics": "^4.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.23",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@capacitor/cli": "^2.0.1",
    "@ionic/angular-toolkit": "^2.1.1",
    "@ionic/lab": "3.1.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cordova-res": "^0.14.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "description": "My Custom Life Application",
  "cordova": {
    "plugins": {}
  }
}
